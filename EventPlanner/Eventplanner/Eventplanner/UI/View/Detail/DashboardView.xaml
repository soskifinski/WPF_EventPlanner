<UserControl x:Class="Eventplanner.UI.View.Detail.DashboardView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Eventplanner.UI.View.Detail"
             xmlns:wrapper="clr-namespace:Eventplanner.UI.Wrapper"
             xmlns:viewModel="clr-namespace:Eventplanner.UI.ViewModel.Detail"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        
        <StackPanel Grid.Column="1" Background="#E0EEEE" Margin="0 50" >
            <Label Grid.Column="1" Content="Übersicht" FontSize="20"  Padding="30 20" Foreground="#708090" FontWeight="Bold"/>
            <Viewbox Grid.Column="0" Grid.Row="2" Margin="0,10,0,10" Grid.ColumnSpan="1" >
                <Calendar Padding="24"/>
            </Viewbox>

        </StackPanel>
    </Grid>
</UserControl>
