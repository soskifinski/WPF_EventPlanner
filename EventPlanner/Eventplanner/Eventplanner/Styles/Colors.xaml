<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="DefaultLightColor">#FFD6DCE5</Color>
    <Color x:Key="DefaultMediumColor">#FFADB9CA</Color>
    <Color x:Key="DefaultDarkColor">#FF8497B0</Color>
    <Color x:Key="DefaultWhiteColor">#FFFFFF</Color>
    <Color x:Key="DefaultBlackColor">#FF181717</Color>
    <Color x:Key="DefaultBackgroundColor">#FFFAFAFA</Color>

    <SolidColorBrush x:Key="DefaultLightBrush" Color="{StaticResource DefaultLightColor}"/>
    <SolidColorBrush x:Key="DefaultMediumBrush" Color="{StaticResource DefaultMediumColor}"/>
    <SolidColorBrush x:Key="DefaultDarkBrush" Color="{StaticResource DefaultDarkColor}"/>
    <SolidColorBrush x:Key="DefaultWhiteBrush" Color="{StaticResource DefaultWhiteColor}"/>
    <SolidColorBrush x:Key="DefaultBlackBrush" Color="{StaticResource DefaultBlackColor}"/>
</ResourceDictionary>